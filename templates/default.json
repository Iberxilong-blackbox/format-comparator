  "numbering_templates": {
    "my_headings": {
      "description": "用于标题的多级编号，格式如 1. / 1.1 / 1.1.1",
      "levels": [
        {
          "level": 1,
          "format_string": "%1",
          "number_style": "arabic",
          "start_at": 1,
          "trailing_char": "space",
          "font": null,
          "indent": {
            "left": "0 cm",
            "hanging": "0.0 cm"
          },
          "alignment": "left"
        },
        {
          "level": 2,
          "format_string": "%1.%2",
          "number_style": "arabic",
          "start_at": 1,
          "trailing_char": "space",
          "font": null,
          "indent": {
            "left": "0 cm",
            "hanging": "0.0 cm"
          },
          "alignment": "left"
        },
        {
          "level": 3,
          "format_string": "%1.%2.%3",
          "number_style": "arabic",
          "start_at": 1,
          "trailing_char": "space",
          "font": null,
          "indent": {
            "left": "0 cm",
            "hanging": "0.0 cm"
          },
          "alignment": "left"
        }
      ]
    }
  },
  "style_numbering_links": {
